{
  "name": "ipld-explorer-components",
  "version": "3.0.3",
  "description": "React components for https://explore.ipld.io",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/ipld-explorer-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/ipld-explorer-components.git"
  },
  "bugs": {
    "url": "https://github.com/ipfs/ipld-explorer-components/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "author": "olizilla",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "breaking": true,
              "release": "major"
            },
            {
              "revert": true,
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "chore",
                "section": "Trivial Changes"
              },
              {
                "type": "docs",
                "section": "Trivial Changes"
              },
              {
                "type": "test",
                "section": "Tests"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=production rollup -c",
    "build:esm": "cross-env NODE_ENV=production babel --quiet src -d dist --copy-files --ignore '**/*.test.js' && tsc",
    "release": "semantic-release",
    "clean": "rm -rf dist",
    "lint": "eslint src/",
    "prepare": "npm run build",
    "dev": "npm run build && vite",
    "start": "cross-env NODE_ENV=production babel src -d dist --copy-files --ignore '**/*.test.js' --watch",
    "storybook": "start-storybook -p 9009 --static-dir public",
    "storybook:build": "build-storybook -c .storybook --static-dir public --output-dir build",
    "test": "react-scripts test --env=./custom-jest-env.js --transformIgnorePatterns '/node_modules/(?!(multiformats/cid)/)'",
    "test:explore.ipld": "aegir test-dependant https://github.com/ipld/explore.ipld.io.git",
    "test:webui": "aegir test-dependant https://github.com/ipfs/ipfs-webui.git"
  },
  "dependencies": {
    "@babel/cli": "^7.13.14",
    "@chainsafe/libp2p-gossipsub": "^8.0.0",
    "@ipld/car": "^5.1.1",
    "@ipld/dag-cbor": "^9.0.0",
    "@ipld/dag-json": "^10.0.1",
    "@libp2p/ipni-content-routing": "^1.0.1",
    "@loadable/component": "^5.14.1",
    "@tableflip/react-inspector": "^2.3.0",
    "blockcodec-to-ipld-format": "^2.0.0",
    "cytoscape": "^3.18.1",
    "cytoscape-dagre": "^2.3.2",
    "filesize": "^6.1.0",
    "ipfs-unixfs": "^4.0.1",
    "ipld": "0.29.0",
    "ipld-ethereum": "6.0.0",
    "ipld-git": "0.6.6",
    "ipld-raw": "7.0.0",
    "milliseconds": "^1.0.3",
    "multiformats": "^9.9.0",
    "react-joyride": "^2.3.0",
    "stream-to-it": "^0.2.4",
    "web-encoding": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
